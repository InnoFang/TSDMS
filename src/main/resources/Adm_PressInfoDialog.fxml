<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="465.0" prefWidth="786.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdmPressInfoDialogController">
    <children>
        <TableView fx:id="pressTable" layoutX="20.0" layoutY="16.0" prefHeight="434.0" prefWidth="444.0" style="-fx-padding: 5; -fx-table-header-border-color: transparent; -fx-table-cell-border-color: transparent; -fx-base: #C5CAE9;">
            <columns>
                <TableColumn fx:id="noColumn" prefWidth="85.0" style="-fx-alignment: CENTER;" text="出版社编号" />
                <TableColumn fx:id="nameColumn" prefWidth="213.0" style="-fx-alignment: CENTER;" text="出版社名" />
                <TableColumn fx:id="telColumn" prefWidth="137.0" style="-fx-alignment: CENTER;" text="联系方式" />
            </columns>
        </TableView>
        <JFXTextField fx:id="noInput" labelFloat="true" layoutX="521.0" layoutY="147.0" prefHeight="30.0" prefWidth="230.0" promptText="出版社编号">
            <font>
                <Font name="Microsoft YaHei UI Light" size="15.0" />
            </font>
        </JFXTextField>
        <JFXTextField fx:id="nameInput" labelFloat="true" layoutX="521.0" layoutY="211.0" prefHeight="30.0" prefWidth="230.0" promptText="出版社名">
            <font>
                <Font name="Microsoft YaHei UI Light" size="15.0" />
            </font>
        </JFXTextField>
        <JFXTextField fx:id="telInput" labelFloat="true" layoutX="521.0" layoutY="275.0" prefHeight="30.0" prefWidth="230.0" promptText="联系方式">
            <font>
                <Font name="Microsoft YaHei UI Light" size="15.0" />
            </font>
        </JFXTextField>
        <JFXButton fx:id="okButton" buttonType="RAISED" layoutX="521.0" layoutY="370.0" onAction="#onHandleOk" prefHeight="30.0" prefWidth="103.0" style="-fx-background-color: #FF8A65;" text="确  认" textFill="WHITE" />
        <JFXButton fx:id="cancelButton" buttonType="RAISED" layoutX="654.0" layoutY="370.0" onAction="#onHandleCancel" prefHeight="30.0" prefWidth="97.0" style="-fx-background-color: #DCE775;" text="取  消" textFill="WHITE" />
      <Label fx:id="message" layoutX="521.0" layoutY="423.0" prefHeight="20.0" prefWidth="230.0" text="Label" />
    </children>
</AnchorPane>
